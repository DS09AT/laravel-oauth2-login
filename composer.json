{
    "name": "kronthto/laravel-oauth2-login",
    "description": "Provides a middleware to protect resources requiring an OAuth2 login",
    "keywords": [
        "OAuth2",
        "Laravel",
        "Middleware",
        "ACL",
        "Login",
        "Session"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Kronthaler",
            "email": "kronthto@gmail.com"
        }
    ],
    "homepage": "https://github.com/kronthto/laravel-oauth2-login",
    "require": {
        "php": "^8.0",
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "laravel/framework": ">=6.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kronthto\\LaravelOAuth2Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kronthto\\LaravelOAuth2Login\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
