{
    "name": "kronthto/laravel-oauth2-login",
    "description": "Provides a middleware to protect ressources requiring an OAuth2 login",
    "keywords": [
        "OAuth2",
        "Laravel",
        "Middleware",
        "ACL",
        "Login",
        "Session"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Kronthaler",
            "email": "kronthto@gmail.com"
        }
    ],
    "homepage": "https://github.com/kronthto/laravel-oauth2-login",
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.3.x|5.4.x",
        "league/oauth2-client": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Kronthto\\LaravelOAuth2Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
